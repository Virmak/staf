TEST SUITE "Module Achats"
IMPORTS
    IMPORT selenium
    IMPORT logger
    IMPORT ws
    IMPORT "../authentication/main.staf"
    IMPORT "./pages/order.page"
    IMPORT "./pages/add_order.page"
VARS
    $googleUrl = 'https://google.com'
    
TEST CASES
    SETUP:
        Open browser                    ('chrome')
        Maximize browser window()
    END

    Test go to purchase page:
        GIVEN   user login()
        WHEN    Open Order list page()
        THEN    Order list page opened()
    END
    
    Test go to add order page:
        GIVEN   Order list page opened()
        WHEN    Click add order button()
        THEN    Add order page opened()
    END

    Test add order:
        GIVEN   Add order page opened()
        WHEN    Select tier ("Tiers 15")
        AND     Log("ok")

    END

    TEARDOWN:
    END